package fr.ensma.a3.ia.agentcase;

public class PresentationCase implements IPresentationCase {
	
	IPresentationCase mediateur;
	
	public PresentationCase() {
		mediateur = new MediateurCase(this);
	}

	
	@Override
	public VueCase getVue() {
		return mediateur.getVue();
	}


	@Override
	public void setCase() {
		mediateur.setCase();
	}
	
}
